import numpy as np
m1 = np.array([[5, -10, 15], [3, -6, 9], [-4, 8, 12]])
m2 = np.array([[7, -12, 18], [5, -8, 11], [-6, 10, 14]])
m3 = m1 + m2
m4 = m1 - m2
m5 = m1 * m2
m6 = m1 / m2
print("m1 is")
print(m1)
print("m2 is")
print(m2)
print("addition")
print(m3)
print("subtraction")
print(m4)
print("multiplication")
print(m5)
print("divition")
print(m6)
